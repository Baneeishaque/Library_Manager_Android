<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_login"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".Map"
    android:clickable="false"
    android:background="@drawable/floortile">


    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_width="70dp">

            <Button
                android:layout_height="100dp"
                android:id="@+id/r12"
                android:layout_width="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/r5"
                android:layout_alignStart="@+id/r5"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_height="100dp"
                android:id="@+id/r11"
                android:layout_width="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/r5"
                android:layout_alignStart="@+id/r5"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_height="100dp"
                android:id="@+id/r10"
                android:layout_width="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/r5"
                android:layout_alignStart="@+id/r5"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_height="100dp"
                android:id="@+id/r9"
                android:layout_width="50dp"
                android:layout_above="@+id/r6"
                android:layout_alignLeft="@+id/r4"
                android:layout_alignStart="@+id/r4"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

        </LinearLayout>
        <Space
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="50"
            android:elevation="1dp" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_width="70dp">

            <Button
                android:layout_height="100dp"
                android:id="@+id/r8"
                android:layout_width="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/r5"
                android:layout_alignStart="@+id/r5"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_height="100dp"
                android:id="@+id/r7"
                android:layout_width="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/r5"
                android:layout_alignStart="@+id/r5"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_height="100dp"
                android:id="@+id/r6"
                android:layout_width="50dp"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/r5"
                android:layout_alignStart="@+id/r5"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_height="100dp"
                android:id="@+id/r5"
                android:layout_width="50dp"
                android:layout_above="@+id/r6"
                android:layout_alignLeft="@+id/r4"
                android:layout_alignStart="@+id/r4"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

        </LinearLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="50"
            android:elevation="1dp" />

        <LinearLayout
            android:orientation="vertical"
            android:layout_height="match_parent"
            android:weightSum="1"
            android:layout_width="70dp">

            <Button
                android:layout_height="100dp"
                android:id="@+id/r4"
                android:layout_width="50dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_width="50dp"
                android:layout_height="100dp"
                android:id="@+id/r3"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/r2"
                android:layout_alignStart="@+id/r2"
                android:onClick="shelf"
                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_width="50dp"
                android:layout_height="100dp"
                android:id="@+id/r2"
                android:layout_alignBaseline="@+id/r5"
                android:layout_alignBottom="@+id/r5"
                android:layout_alignLeft="@+id/r1"
                android:layout_alignStart="@+id/r1"
                android:onClick="shelf"

                android:clickable="false"
                android:backgroundTint="@color/colorPrimary" />

            <Button
                android:layout_width="50dp"
                android:id="@+id/r1"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:onClick="shelf"
                android:clickable="false"
                android:layout_height="100dp"
                android:backgroundTint="@color/colorPrimary"
                android:elevation="0dp" />
        </LinearLayout>

    </LinearLayout>

</RelativeLayout>
